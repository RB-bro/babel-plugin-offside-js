{
  "name": "babel-plugin-offside-js",
  "version": "0.4.0",
  "description": "Babel offside (indention) Javascript syntax extension",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "license": "BSD-2-Clause",
  "keywords": [
    "babel",
    "syntax",
    "offside",
    "python",
    "indention",
    "indent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanewholloway/babel-plugin-offside-js.git"
  },
  "bugs": {
    "url": "https://github.com/shanewholloway/babel-plugin-offside-js/issues"
  },
  "homepage": "https://github.com/shanewholloway/babel-plugin-offside-js#readme",
  "main": "dist/index.js",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "tap-lite-tester": "^0.3.9"
  },
  "babel": {
    "plugins": [
      "./dist/stable"
    ]
  },
  "scripts": {
    "watch": "npm -s run build -- --watch",
    "build": "babel --source-maps true code -d dist",
    "build-examples": "babel --source-maps true test/ex* -d tmp",
    "predebug": "npm -s run build",
    "debug": "OFFSIDE_DEBUG=true node test/testSimple.js",
    "pretest": "npm -s run build",
    "test": "node test/testSimple.js"
  }
}
